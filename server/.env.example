# Database
DATABASE_URL="postgresql://user:password@localhost:5432/tradetracker?schema=public"

# Supabase
SUPABASE_URL="your-supabase-url"
SUPABASE_SERVICE_KEY="your-supabase-service-key"

# Upstash Redis
UPSTASH_REDIS_REST_URL="your-upstash-redis-rest-url"
UPSTASH_REDIS_REST_TOKEN="your-upstash-redis-rest-token"
UPSTASH_REDIS_REGION="your-upstash-redis-region" # e.g., eu-west-1

# Clerk Authentication
CLERK_SECRET_KEY="your-clerk-secret-key"
CLERK_PUBLISHABLE_KEY="your-clerk-publishable-key"

# Stripe
STRIPE_SECRET_KEY="your-stripe-secret-key"
STRIPE_WEBHOOK_SECRET="your-stripe-webhook-secret"

# Google AI
GOOGLE_AI_API_KEY="your-google-ai-api-key"

# Security
ENCRYPTION_KEY="your-32-byte-encryption-key" # For encrypting broker credentials in database

# Feature Flags
ENABLE_DEMO_MODE="true"      # Enable demo trading functionality
ENABLE_PAPER_TRADING="true"   # Enable paper trading functionality

# Application
NODE_ENV="development"        # development, production, or test
PORT="3001"                  # Server port
LOG_LEVEL="info"             # debug, info, warn, or error
